{
    "name": "santhika29/yii2-admin",
    "description": "RBAC Auth manager for Yii2 with adjustment to bootstrap 4",
    "keywords": ["yii", "admin", "auth", "rbac"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/santhika29/yii2-admin/issues",
        "source": "https://github.com/santhika29/yii2-admin"
    },  
    "authors": [
        {
            "name": "Misbahul Munir",
            "email": "misbahuldmunir@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0.14"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap4": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
    },
    "autoload": {
        "psr-4": {
            "sant\\admin\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
