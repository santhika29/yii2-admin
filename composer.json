{
    "name": "santhika29/yii2-admin-bs4",
    "description": "RBAC Auth manager for Yii2 with adjustment to bootstrap 4",
    "keywords": ["yii", "admin", "auth", "rbac"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sant",
            "email": "sant@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.14",
        "kartik-v/yii2-grid": "*"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap4": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
    },
    "autoload": {
        "psr-4": {
            "sant\\admin\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
